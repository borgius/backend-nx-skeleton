{
  "name": "@webundsoehne/nx-nest",
  "version": "1.0.0",
  "description": "Nest Plugin for Nx",
  "repository": "https://gitlab.tailored-apps.com/ckilic/nx-test",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "author": "Backend <backend@webundsoehne.com>",
  "license": "ISC",
  "publishConfig": {
    "access": "public"
  },
  "schematics": "./schematics.json",
  "scripts": {
    "prebuild": "rimraf tsconfig*.tsbuildinfo && rimraf dist/",
    "build": "tsc -p tsconfig.build.json",
    "postbuild": "tscpaths -p tsconfig.paths.json -s ./dist -o ./dist && cpy --cwd=src --parents '**/*.json' ../dist/ && rimraf dist/**/files/ && cpy --cwd=src --parents '**/files/**' ../dist/",
    "dev:build": "yarn prebuild && tsc -p tsconfig.json && yarn postbuild",
    "dev:start": "yarn dev:build && TS_NODE_PROJECT='tsconfig.paths.json' tsc-watch --noClear --onSuccess \"yarn postbuild\"",
    "lint": "prettier --loglevel warn --write src/ && eslint --ext .ts,.js,.tsx,.jsx --fix src/",
    "lint:check": "eslint --ext .ts,.js,.tsx,.jsx src/",
    "lint:staged": "lint-staged"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "prepare-commit-msg": "exec < /dev/tty && git cz --hook || true"
    }
  },
  "lint-staged": {
    "./*.{ts,js,tsx,jsx}": ["prettier --loglevel warn --write", "eslint --fix"],
    "./*.{json,md}": ["prettier --loglevel warn --write"]
  },
  "peerDependencies": {
    "@nrwl/workspace": "*",
    "@webundsoehne/nx-builders": "*"
  },
  "dependencies": {
    "@angular-devkit/schematics": "~10.1.2",
    "@nestjs/schematics": "^7.1.2",
    "@webundsoehne/nx-builders": "*",
    "@webundsoehne/nx-tools": "*",
    "chalk": "^4.1.0",
    "change-case": "^4.1.1",
    "deepmerge": "^4.2.2",
    "enquirer": "^2.3.5",
    "execa": "^4.0.2",
    "listr2": "^3.1.1"
  },
  "devDependencies": {
    "@types/deepmerge": "^2.2.0",
    "cpy-cli": "^3.1.1",
    "lint-staged": "^10.2.11",
    "rimraf": "^3.0.2",
    "tsc-watch": "^4.2.8",
    "tscpaths": "^0.0.9",
    "typescript": "^4.0.3"
  }
}
