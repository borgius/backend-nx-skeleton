{
  "name": "@webundsoehne/nx-nest",
  "version": "0.1",
  "extends": ["@nrwl/workspace"],
  "schematics": {
    "init": {
      "factory": "./dist/schematics/application/init",
      "schema": "./dist/schematics/application/schema.json",
      "description": "Initialize the @webundsoehne/nx-nest plugin.",
      "aliases": ["ng-add"],
      "hidden": true
    },

    "application": {
      "factory": "./dist/schematics/application/main",
      "schema": "./dist/schematics/application/schema.json",
      "aliases": ["app"],
      "description": "Create a new Nest application from skeleton."
    },

    "command": {
      "factory": "./dist/schematics/command/main",
      "schema": "./dist/schematics/command/schema.json",
      "aliases": ["cmd"],
      "description": "Add command module to skeleton."
    }
  }
}