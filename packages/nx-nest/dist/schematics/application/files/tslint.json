{
  "defaultSeverity": "error",
  "extends": [
    "tslint:recommended"
  ],
  "jsRules": {
    "no-unused-expression": true
  },
  "rules": {
    "quotemark": [
      true,
      "single"
    ],
    "member-access": [
      false
    ],
    "ordered-imports": [
      false
    ],
    "max-line-length": [
      false
    ],
    "member-ordering": [
      false
    ],
    "interface-name": [
      false
    ],
    "arrow-parens": true,
    "object-literal-sort-keys": false,
    "semicolon": false,
    "trailing-comma": false,
    "max-classes-per-file": false,
    "variable-name": false,
    "space-before-function-paren": true,
    "no-unused-expression": false
  },
  "rulesDirectory": []
}
