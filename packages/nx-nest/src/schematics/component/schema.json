{
  "$schema": "http://json-schema.org/schema",
  "id": "nx-nest:component",
  "title": "Add a component to existing nx-nest schematic.",
  "examples": [],
  "type": "object",
  "properties": {
    "parent": {
      "description": "The name of parent application.",
      "type": "string",
      "$default": {
        "$source": "argv",
        "index": 0
      },
      "x-prompt": "What parent application do you want to run on?"
    },
    "name": {
      "description": "The name of the component.",
      "type": "string",
      "$default": {
        "$source": "argv",
        "index": 1
      },
      "x-prompt": "What name do you want to give to your module?"
    },
    "force": {
      "description": "Do not prompt for overwrites.",
      "type": "boolean"
    }
  },
  "required": []
}