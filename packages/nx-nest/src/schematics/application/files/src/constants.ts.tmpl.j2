export enum service {
  {% if 'server' in components %}server = 'server',{% endif %}
  <% if (components.includes('microservice')) { %>microservice = 'microservice',<% } %>
  <% if (components.includes('bgtask')) { %>task = 'bgtask',<% } %>
  <% if (components.includes('command')) { %>command = 'cli'<% } %>
}

export enum logType {
  error = 'error',
  log = 'info',
  warn = 'warn',
  debug = 'debug',
  verbose = 'verbose'
}
