{
  "$schema": "http://json-schema.org/schema",
  "$id": "nx-nest:generate",
  "title": "Add a single file to existing nx-nest schematic.",
  "examples": [],
  "type": "object",
  "properties": {
    "name": {
      "description": "The name of the generated partial.",
      "type": "string",
      "$default": {
        "$source": "argv",
        "index": 0
      }
    },
    "type": {
      "description": "The type of the generated component.",
      "type": "string",
      "$default": {
        "$source": "argv",
        "index": 1
      }
    },
    "directory": {
      "description": "Directory where the component is getting generated.",
      "type": "string",
      "$default": {
        "$source": "argv",
        "index": 2
      }
    },
    "force": {
      "description": "Do not prompt for overwrites.",
      "type": "boolean"
    }
  },
  "required": []
}
