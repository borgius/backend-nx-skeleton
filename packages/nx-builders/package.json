{
  "name": "@webundsoehne/nx-builders",
  "version": "1.0.0",
  "description": "TSC compiler for NX.",
  "repository": "https://gitlab.tailored-apps.com/ckilic/nx-test",
  "author": "Backend <backend@webundsoehne.com>",
  "license": "ISC",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "publishConfig": {
    "access": "public"
  },
  "schematics": "./schematics.json",
  "builders": "./builders.json",
  "scripts": {
    "prebuild": "rimraf tsconfig*.tsbuildinfo && rimraf dist/",
    "build": "tsc -p tsconfig.build.json",
    "postbuild": "tscpaths -p tsconfig.paths.json -s ./dist -o ./dist && cpy --cwd=src --parents --dot '**/*.json' ../dist/",
    "dev:build": "yarn prebuild && tsc -p tsconfig.json && tscpaths -p tsconfig.paths.json -s ./dist -o ./dist",
    "dev:start": "yarn dev:build && tsc-watch --noClear --onSuccess \"yarn postbuild\"",
    "lint": "prettier --loglevel warn --write src/ && eslint --ext .ts,.js,.tsx,.jsx --fix src/",
    "lint:check": "eslint --ext .ts,.js,.tsx,.jsx src/",
    "lint:staged": "lint-staged",
    "docs:jsdoc": "jsdoc2md --files './src/**' --configure './jsdoc2md.json' > ./API.md",
    "docs:toc": "markdown-toc README.md --bullets='-' -i"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "prepare-commit-msg": "exec < /dev/tty && git cz --hook || true"
    }
  },
  "lint-staged": {
    "./**/*.{ts,js,tsx,jsx}": ["prettier --loglevel warn --write", "eslint --fix"],
    "./**/*.{json,md}": ["prettier --loglevel warn --write"]
  },
  "peerDependencies": {
    "@nrwl/workspace": "*",
    "ts-node": ">= 8.10.0",
    "ts-node-dev": ">= 1.0.0",
    "tsc-watch": ">= 4.2.0",
    "tscpaths": ">= 0.0.9",
    "typescript": ">= 3.9.0"
  },
  "dependencies": {
    "@angular-devkit/architect": "^0.1102.2",
    "@angular-devkit/core": "^11.2.2",
    "@angular-devkit/schematics": "^11.2.2",
    "@nrwl/cli": "^11.4.0",
    "@webundsoehne/nx-tools": "*",
    "delay": "^5.0.0",
    "execa": "^5.0.0",
    "tree-kill": "^1.2.2"
  },
  "devDependencies": {
    "@types/execa": "^2.0.0",
    "@types/tsc-watch": "^4.2.0"
  }
}
