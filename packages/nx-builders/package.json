{
  "name": "@webundsoehne/nx-builders",
  "version": "1.0.0",
  "description": "TSC compiler for NX.",
  "repository": "https://gitlab.tailored-apps.com/ckilic/nx-test",
  "author": "Backend <backend@webundsoehne.com>",
  "license": "ISC",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "publishConfig": {
    "access": "public"
  },
  "schematics": "./schematics.json",
  "builders": "./builders.json",
  "scripts": {
    "prebuild": "rimraf tsconfig*.tsbuildinfo && rimraf dist/",
    "build": "tsc -p tsconfig.build.json",
    "postbuild": "tsconfig-replace-paths --project tsconfig.json && cpy --cwd=src --parents --dot '**/*.json' ../dist/",
    "dev:build": "yarn prebuild && tsc -p tsconfig.json && tsconfig-replace-paths --project tsconfig.json",
    "dev:start": "yarn dev:build && tsc-watch --noClear --onSuccess \"yarn postbuild\"",
    "lint": "prettier --loglevel warn --write src/ && eslint --ext .ts,.js,.tsx,.jsx --fix src/",
    "lint:check": "eslint --ext .ts,.js,.tsx,.jsx src/",
    "lint:staged": "lint-staged",
    "docs:toc": "markdown-toc README.md --bullets='-' -i",
    "docs:jsdoc": "typedoc src/index.ts --readme none"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "prepare-commit-msg": "exec < /dev/tty && git cz --hook || true"
    }
  },
  "lint-staged": {
    "./**/*.{ts,js,tsx,jsx}": ["prettier --loglevel warn --write", "eslint --fix"],
    "./**/*.{json,md}": ["prettier --loglevel warn --write"]
  },
  "peerDependencies": {
    "@nrwl/workspace": "*",
    "ts-node": ">= 8.10.0",
    "ts-node-dev": ">= 1.0.0",
    "tsc-watch": ">= 4.2.0",
    "tsconfig-replace-paths": "^0.0.11",
    "typescript": ">= 3.9.0"
  },
  "dependencies": {
    "@angular-devkit/architect": "^0.1302.0",
    "@angular-devkit/core": "^13.2.0",
    "@angular-devkit/schematics": "^13.2.0",
    "@nrwl/cli": "^13.6.1",
    "@webundsoehne/deep-merge": "*",
    "@webundsoehne/nx-tools": "*",
    "delay": "^5.0.0",
    "execa": "^5.1.1",
    "tree-kill": "^1.2.2"
  },
  "devDependencies": {
    "@types/execa": "^2.0.0",
    "@types/tsc-watch": "^4.2.0"
  }
}
