{
  "name": "@webundsoehne/nx-tools",
  "extends": ["@nrwl/workspace"],
  "schematics": {
    "exports": {
      "factory": "./dist/schematics/exports/main",
      "schema": "./dist/schematics/exports/schema.json",
      "aliases": ["ex"],
      "description": "Export files a default module with regex patterns."
    },

    "package-script": {
      "factory": "./dist/schematics/package-script/main",
      "schema": "./dist/schematics/package-script/schema.json",
      "aliases": ["script"],
      "description": "Run a package.json script."
    }
  }
}
