{
  "name": "@webundsoehne/nestjs-graphql-typeorm-dataloader",
  "description": "A library to utilize the graphql-dataloader with nestjs and typeorm.",
  "version": "1.0.0",
  "author": "Backend <backend@webundsoehne.com>",
  "license": "ISC",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "require": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "repository": "https://gitlab.tailored-apps.com/ckilic/nx-test",
  "publishConfig": {
    "access": "public"
  },
  "keywords": [
    "nestjs",
    "graphql",
    "typeorm",
    "dataloader",
    "data-loader"
  ],
  "scripts": {
    "build": "tsup-node",
    "dev:start": "tsup-node --watch",
    "lint": "prettier --loglevel warn --write src/ && eslint --ext .ts,.js,.tsx,.jsx --fix src/",
    "lint:check": "eslint --ext .ts,.js,.tsx,.jsx src/",
    "lint:staged": "lint-staged",
    "docs:jsdoc": "typedoc src/index.ts --readme none",
    "docs:toc": "markdown-toc README.md --bullets='-' -i"
  },
  "lint-staged": {
    "./**/*.{ts,js,tsx,jsx}": [
      "prettier --loglevel warn --write",
      "eslint --fix"
    ],
    "./**/*.{json,md}": [
      "prettier --loglevel warn --write"
    ]
  },
  "dependencies": {
    "apollo-server-plugin-base": "^3.5.1",
    "dataloader": "^2.0.0",
    "lodash": "^4.17.21",
    "reflect-metadata": "^0.1.13",
    "typedi": "^0.10.0",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@apollo/gateway": "^0.46.0",
    "@nestjs/graphql": "^9.1.2",
    "@types/jest": "^27.4.0",
    "@types/lodash": "^4.14.178",
    "@types/node": "^17.0.13",
    "@types/uuid": "^8.3.4",
    "apollo-server-fastify": "^3.6.2",
    "class-validator": "^0.13.2",
    "graphql": "^15",
    "graphql-request": "^3.7.0",
    "mikro-orm": "^4.5.10",
    "rimraf": "^3.0.2",
    "sqlite3": "^5.0.2",
    "typeorm": "^0.2.41"
  },
  "engines": {
    "node": ">= 16"
  }
}
