{
  "name": "publish",
  "private": true,
  "version": "1.0.0",
  "description": "Publisher for this repository.",
  "repository": "https://gitlab.tailored-apps.com/ckilic/nx-test",
  "main": "index.js",
  "author": "Backend <backend@webundsoehne.com>",
  "license": "ISC",
  "schematics": "./schematics.json",
  "scripts": {
    "lint": "prettier --loglevel warn --write . && eslint --ext .ts,.js,.tsx,.jsx --fix .",
    "lint:check": "eslint --ext .ts,.js,.tsx,.jsx .",
    "lint:staged": "lint-staged"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "prepare-commit-msg": "exec < /dev/tty && git cz --hook || true"
    }
  },
  "lint-staged": {
    "./**/*.{ts,js,tsx,jsx}": ["prettier --loglevel warn --write", "eslint --fix"],
    "./**/*.{json,md}": ["prettier --loglevel warn --write"]
  },
  "dependencies": {},
  "devDependencies": {
    "@semantic-release/npm": "^7.0.8"
  }
}
