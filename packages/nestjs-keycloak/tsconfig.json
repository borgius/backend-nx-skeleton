{
  "extends": "../../tsconfig.json",
  "compilerOptions": {
    "outDir": "./dist",
    "rootDir": "src",
    "baseUrl": "src",
    "tsBuildInfoFile": "tsconfig.tsbuildinfo",
    "paths": {
      "@constants/*": ["constants/*"],
      "@interfaces/*": ["interfaces/*"],
      "@admin/*": ["admin/*"],
      "@connect/*": ["connect/*"]
    }
  },
  "include": ["src"],
  "exclude": ["node_modules", "dist"]
}
