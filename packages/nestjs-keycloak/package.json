{
  "name": "@webundsoehne/nestjs-keycloak",
  "description": "Keycloak modules for token verification and administration of Keycloak.",
  "version": "1.0.0",
  "author": "Backend <backend@webundsoehne.com>",
  "license": "ISC",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "repository": "https://gitlab.tailored-apps.com/ckilic/nx-test",
  "publishConfig": {
    "access": "public"
  },
  "keywords": [
    "nestjs",
    "keycloak"
  ],
  "scripts": {
    "prebuild": "rimraf tsconfig*.tsbuildinfo && rimraf dist/",
    "build": "tsc -p tsconfig.build.json",
    "postbuild": "tsconfig-replace-paths --project tsconfig.json",
    "dev:build": "yarn prebuild && tsc -p tsconfig.json && yarn postbuild",
    "dev:start": "yarn dev:build && tsc-watch --noClear --onSuccess \"yarn postbuild\"",
    "lint": "prettier --loglevel warn --write src/ && eslint --ext .ts,.js,.tsx,.jsx --fix src/",
    "lint:check": "eslint --ext .ts,.js,.tsx,.jsx src/",
    "lint:staged": "lint-staged",
    "docs:jsdoc": "typedoc src/index.ts src/graphql.ts src/restful.ts --readme none",
    "docs:toc": "markdown-toc README.md --bullets='-' -i"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "prepare-commit-msg": "exec < /dev/tty && git cz --hook || true"
    }
  },
  "lint-staged": {
    "./**/*.{ts,js,tsx,jsx}": [
      "prettier --loglevel warn --write",
      "eslint --fix"
    ],
    "./**/*.{json,md}": [
      "prettier --loglevel warn --write"
    ]
  },
  "engines": {
    "node": ">=12.0.0"
  },
  "dependencies": {},
  "devDependencies": {
    "@keycloak/keycloak-admin-client": "^16.1.1",
    "@nestjs/common": "^8.2.6",
    "@nestjs/core": "^8.2.6",
    "@nestjs/graphql": "^9.0.4",
    "express": "^4.17.2",
    "fastify": "^3.27.0",
    "graphql": "^15",
    "keycloak-connect": "^16.1.1",
    "@types/express": "^4.17.3"
  },
  "peerDependencies": {
    "@keycloak/keycloak-admin-client": ">= 15",
    "@nestjs/common": ">= 8",
    "@nestjs/core": ">= 8",
    "@nestjs/graphql": ">= 8",
    "express": ">= 4",
    "fastify": ">= 3",
    "graphql": ">= 15",
    "keycloak-connect": ">= 15"
  },
  "peerDependenciesMeta": {
    "graphql": {
      "optional": true
    },
    "express": {
      "optional": true
    },
    "fastify": {
      "optional": true
    },
    "@nestjs/graphql": {
      "optional": true
    }
  }
}
