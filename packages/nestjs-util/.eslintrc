{
  "extends":"../../.eslintrc.js",
  "rules": {
    "import/order": [
      "error",
      {
        "pathGroups": [
          {
            "pattern": "@filter",
            "group": "index"
          },
          {
            "pattern": "@filter/**",
            "group": "index"
          },
          {
            "pattern": "@decorator",
            "group": "index"
          },
          {
            "pattern": "@decorator/**",
            "group": "index"
          },
          {
            "pattern": "@interface",
            "group": "index"
          },
          {
            "pattern": "@module",
            "group": "index"
          },
          {
            "pattern": "@module/**",
            "group": "index"
          },
          {
            "pattern": "@provider",
            "group": "index"
          },
          {
            "pattern": "@provider/**",
            "group": "index"
          },
          {
            "pattern": "@util",
            "group": "index"
          }
        ],
        "pathGroupsExcludedImportTypes": ["builtin"],
        "groups": [
          ["builtin", "external"],
          ["index", "parent", "sibling"]
        ],
        "newlines-between": "always",
        "alphabetize": {
          "order": "asc",
          "caseInsensitive": true
        }
      }
    ]
  }
}