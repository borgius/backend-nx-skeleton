{
  "name": "@webundsoehne/nestjs-util",
  "version": "1.0.0-development",
  "description": "NestJS skeleton util library",
  "author": "Backend <backend@webundsoehne.com>",
  "license": "ISC",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "prebuild": "rimraf tsconfig*.tsbuildinfo && rimraf dist/",
    "build": "tsc -p tsconfig.build.json",
    "dev:build": "yarn prebuild && tsc -p tsconfig.json && tscpaths -p tsconfig.paths.json -s ./dist -o ./dist",
    "dev:start": "yarn dev:build && TS_NODE_PROJECT='tsconfig.paths.json' tsc-watch --noClear --onSuccess \"yarn dev:start:onSuccess\"",
    "dev:start:onSuccess": "tscpaths -p tsconfig.paths.json -s ./dist -o ./dist",
    "lint": "prettier --write src/ && eslint --ext .ts,.js,.tsx,.jsx --fix src/",
    "lint:check": "eslint --ext .ts,.js,.tsx,.jsx src/"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "prepare-commit-msg": "exec < /dev/tty && git cz --hook || true"
    }
  },
  "lint-staged": {
    "./*.{ts,js,tsx,jsx}": [
      "prettier --write",
      "eslint --fix"
    ],
    "./*.{json,md}": [
      "prettier --write"
    ]
  },
  "keywords": [
    "nestjs",
    "util"
  ],
  "dependencies": {
    "config": "^3.2.4",
    "fs-extra": "^9.0.0",
    "moment": "^2.24.0",
    "reflect-metadata": "^0.1.13",
    "winston": "^3.2.1"
  },
  "peerDependencies": {
    "@nestjs/common": ">= 7 < 8",
    "@nestjs/core": ">= 7 < 8",
    "@nestjs/swagger": ">= 4 < 5",
    "fastify": ">= 3.0.0",
    "fastify-swagger": ">= 3.0.0",
    "nest-schedule": ">= 0.6.0"
  },
  "devDependencies": {
    "cz-conventional-changelog": "3.2.0",
    "@nestjs/common": "^7.0.13",
    "@nestjs/core": "^7.0.13",
    "@nestjs/swagger": "^4.5.7",
    "@nestjs/testing": "^7.0.13",
    "@types/jest": "^26.0.5",
    "@types/node": "^14.0.1",
    "@types/supertest": "^2.0.8",
    "@types/winston": "^2.4.4",
    "concurrently": "^5.0.0",
    "fastify": "^3.1.1",
    "fastify-swagger": "^3.1.2",
    "husky": "^4.2.5",
    "jest": "^26.0.1",
    "lint-staged": "^10.2.2",
    "nest-schedule": "^0.6.4",
    "prettier": "^2.0.5",
    "ts-node": "^8.5.3",
    "tsconfig-paths": "^3.9.0",
    "eslint": "^7.5.0",
    "typescript": "^3.9.2",
    "wait-on": "^5.0.0"
  },
  "engines": {
    "node": ">=10"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}