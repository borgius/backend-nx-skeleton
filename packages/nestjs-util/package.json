{
  "name": "@webundsoehne/nestjs-util",
  "version": "4.2.3",
  "description": "NestJS skeleton util library",
  "author": "Backend <backend@webundsoehne.com>",
  "license": "ISC",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "prebuild": "rimraf tsconfig*.tsbuildinfo && rimraf dist/",
    "build": "tsc -p tsconfig.build.json",
    "postbuild": "tsconfig-replace-paths --project tsconfig.json",
    "dev:build": "yarn prebuild && tsc -p tsconfig.json && yarn postbuild",
    "dev:start": "yarn dev:build && tsc-watch --noClear --onSuccess \"yarn postbuild\"",
    "lint": "prettier --loglevel warn --write src/ && eslint --ext .ts,.js,.tsx,.jsx --fix src/",
    "lint:check": "eslint --ext .ts,.js,.tsx,.jsx src/",
    "lint:staged": "lint-staged",
    "docs:toc": "markdown-toc README.md --bullets='-' -i",
    "docs:jsdoc": "typedoc src/index.ts src/microservices.ts src/graphql.ts --readme none"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "prepare-commit-msg": "exec < /dev/tty && git cz --hook || true"
    }
  },
  "lint-staged": {
    "./src/*.{ts,js,tsx,jsx}": [
      "prettier --loglevel warn --write",
      "eslint --fix"
    ],
    "./src/*.{json,md}": [
      "prettier --loglevel warn --write"
    ]
  },
  "keywords": [
    "nestjs",
    "util"
  ],
  "dependencies": {
    "@webundsoehne/deep-merge": "*",
    "config": "^3.3.7",
    "fs-extra": "^10.0.0",
    "js-yaml": "^4.0.0",
    "moment": "^2.29.1",
    "reflect-metadata": "^0.1.13",
    "winston": "^3.4.0"
  },
  "peerDependencies": {
    "@nestjs/common": ">= 8 < 10",
    "@nestjs/core": ">= 8 < 10",
    "@nestjs/graphql": ">= 8 < 10",
    "@nestjs/microservices": ">= 8 < 10",
    "@nestjs/swagger": ">= 5 < 6",
    "class-transformer": ">= 0.4.0",
    "class-validator": ">= 0.13.0",
    "fastify": ">= 3",
    "fastify-swagger": ">= 4.0.0 < 5",
    "graphql": ">= 15 < 16",
    "nest-schedule": ">= 0.6.0"
  },
  "devDependencies": {
    "@nestjs/common": "^8.2.5",
    "@nestjs/core": "^8.2.5",
    "@nestjs/graphql": "^9.1.2",
    "@nestjs/microservices": "^8.2.5",
    "@nestjs/swagger": "^5.1.5",
    "@nestjs/testing": "^8.2.5",
    "@types/config": "^0.0.41",
    "@types/jest": "^27.4.0",
    "@types/node": "^17.0.10",
    "@types/winston": "^2.4.4",
    "amqp-connection-manager": "^4.0.0",
    "amqplib": "^0.8.0",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.13.2",
    "concurrently": "^7.0.0",
    "fastify": "^3.27.0",
    "fastify-swagger": "^4.13.1",
    "graphql": "^15",
    "nest-schedule": "^0.6.4",
    "prettier": "^2.5.1",
    "swagger-ui-express": "^4.1.5",
    "wait-on": "^6.0.0"
  },
  "engines": {
    "node": ">=10"
  }
}
