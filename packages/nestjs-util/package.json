{
  "name": "@webundsoehne/nestjs-util",
  "version": "1.0.0",
  "description": "NestJS skeleton util library",
  "author": "Backend <backend@webundsoehne.com>",
  "license": "ISC",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "publishConfig": {
    "access": "public"
  },
  "keywords": [
    "nestjs",
    "util"
  ],
  "scripts": {
    "build": "tsup-node",
    "dev:start": "tsup-node --watch",
    "lint": "prettier --loglevel warn --write src/ && eslint --ext .ts,.js,.tsx,.jsx --fix src/",
    "lint:check": "eslint --ext .ts,.js,.tsx,.jsx src/",
    "lint:staged": "lint-staged",
    "docs:toc": "markdown-toc README.md --bullets='-' -i",
    "docs:jsdoc": "typedoc src/index.ts --readme none"
  },
  "lint-staged": {
    "*.{ts,js,tsx,jsx}": [
      "prettier --loglevel warn --write",
      "eslint --fix"
    ],
    "*.{json,md}": [
      "prettier --loglevel warn --write"
    ]
  },
  "dependencies": {
    "@webundsoehne/deep-merge": "*",
    "config": "^3.3.7",
    "fs-extra": "^10.0.0",
    "js-yaml": "^4.0.0",
    "moment": "^2.29.1",
    "reflect-metadata": "^0.1.13",
    "winston": "^3.5.0"
  },
  "peerDependencies": {
    "@nestjs/common": ">= 8 < 10",
    "@nestjs/core": ">= 8 < 10",
    "@nestjs/microservices": ">= 8 < 10",
    "class-transformer": ">= 0.4.0",
    "class-validator": ">= 0.13.0",
    "fastify": ">= 3",
    "express": ">= 4 < 5"
  },
  "peerDependenciesMeta": {
    "fastify": {
      "optional": true
    },
    "express": {
      "optional": true
    }
  },
  "devDependencies": {
    "@nestjs/common": "^8.2.6",
    "@nestjs/core": "^8.2.6",
    "@types/config": "^0.0.41",
    "@types/winston": "^2.4.4",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.13.2",
    "fastify": "^3.27.0",
    "express": "^4.17.2",
    "@types/express": "^4.17.3"
  },
  "engines": {
    "node": ">= 16"
  }
}
