{
  "name": "@webundsoehne/ts-utility-types",
  "description": "Some generic utility types that are used in more than one place now.",
  "repository": "https://github.com/tailoredmedia/backend-nx-skeleton",
  "version": "1.0.0",
  "author": "Backend <backend@webundsoehne.com>",
  "license": "ISC",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "publishConfig": {
    "access": "public"
  },
  "keywords": [
    "nestjs",
    "keycloak"
  ],
  "scripts": {
    "build": "tsup-node",
    "dev:start": "tsup-node --watch",
    "lint": "prettier --loglevel warn --write src/ && eslint --ext .ts,.js,.tsx,.jsx --fix src/",
    "lint:check": "eslint --ext .ts,.js,.tsx,.jsx src/",
    "docs:jsdoc": "typedoc src/index.ts --readme none",
    "docs:toc": "markdown-toc README.md --bullets='-' -i"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "prepare-commit-msg": "exec < /dev/tty && git cz --hook || true"
    }
  },
  "lint-staged": {
    "*.{ts,js,tsx,jsx}": [
      "prettier --loglevel warn --write",
      "eslint --fix"
    ],
    "*.{json,md}": [
      "prettier --loglevel warn --write"
    ]
  },
  "dependencies": {},
  "devDependencies": {},
  "engines": {
    "node": ">= 16"
  }
}
