{
  "extends": "../../tsconfig.json",
  "compilerOptions": {
    "outDir": "./dist",
    "rootDir": "src",
    "baseUrl": "src",
    "paths": {
      "@constants/*": ["constants/*"],
      "@decorators/*": ["decorators/*"],
      "@interfaces/*": ["interfaces/*"],
      "@utils/*": ["utils/*"],
      "@module/*": ["module/*"]
    }
  },
  "include": ["src"],
  "exclude": ["node_modules", "dist"]
}
